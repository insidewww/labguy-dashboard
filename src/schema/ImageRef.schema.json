{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "etag": {
      "type": "string"
    },
    "public_id": {
      "type": "string"
    },
    "mediaType": {
      "type": "string",
      "default": "IMAGE",
      "enum": ["IMAGE", "VIDEO", "THREE_D"]
    },
    "cld_url": {
      "type": "string"
    },
    "path": {
      "type": "string"
    },
    "filename": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "bytes": {
      "type": "integer"
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "width": {
      "type": "integer"
    },
    "height": {
      "type": "integer"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time"
    }
  }
}
