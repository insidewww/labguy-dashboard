{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ImageInstance",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "etag": {
      "type": "string"
    },
    "public_id": {
      "type": "string"
    },
    "original_filename": {
      "type": "string"
    },
    "filename": {
      "type": "string"
    },
    "path": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "dimensions": {
      "type": "object",
      "properties": {
        "height": {
          "type": "number"
        },
        "width": {
          "type": "number"
        }
      },
      "required": ["height", "width"]
    },
    "bytes": {
      "type": "number"
    },
    "url": {
      "type": "string"
    },
    "secure_url": {
      "type": "string"
    },
    "cld_url": {
      "type": "string"
    },
    "cld_secure_url": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "alt": {
      "type": "string"
    },
    "timestamp": {
      "type": "string"
    },
    "modified": {
      "type": "string"
    }
  },
  "required": [
    "type",
    "etag",
    "public_id",
    "original_filename",
    "filename",
    "path",
    "format",
    "dimensions",
    "bytes",
    "url",
    "secure_url",
    "cld_url",
    "cld_secure_url",
    "tags",
    "alt",
    "timestamp",
    "modified"
  ]
