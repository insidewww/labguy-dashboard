{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "VideoInstance",
  "type": "object",
  "properties": {
    "type": {
      "type": "string"
    },
    "etag": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "thumbnails": {
      "type": "object",
      "properties": {
        "default": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            }
          },
          "required": ["url", "width", "height"]
        },
        "medium": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            }
          },
          "required": ["url", "width", "height"]
        },
        "high": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            }
          },
          "required": ["url", "width", "height"]
        },
        "standard": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            }
          },
          "required": ["url", "width", "height"]
        },
        "maxres": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "width": {
              "type": "number"
            },
            "height": {
              "type": "number"
            }
          },
          "required": ["url", "width", "height"]
        }
      },
      "required": ["default", "medium", "high", "standard", "maxres"]
    },
    "description": {
      "type": "string"
    },
    "publishedAt": {
      "type": "string"
    },
    "duration": {
      "type": "string"
    },
    "definition": {
      "type": "string"
    }
  },
  "required": [
    "type",
    "etag",
    "title",
    "tags",
    "thumbnails",
    "description",
    "publishedAt",
    "duration",
    "definition",
    "timestamp",
    "modified"
  ]
}
